{
  "name": "cleanstr",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "author": "Verse",
  "repository": {
    "type": "git",
    "url": "https://github.com:planetary-social/cleanstr.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "c8 mocha -p -j 2 test/*.test.js --timeout=5000",
    "test:integration": "c8 mocha -p -j 2 test/integration.test.js --timeout=5000",
    "test:grep": "c8 mocha -p -j 2 --timeout=5000 --grep",
    "test:rateLimit": "node runArtilleryTest.mjs",
    "logs": "gcloud functions logs read nodejs-moderator-function --gen2",
    "sendTestEvent": "./bin/sendTestEvent.sh",
    "deploy": "gcloud functions deploy nodejs-moderator-function --gen2 --runtime=nodejs20 --region=us-central1 --trigger-topic nostr-events --source=. --entry-point=nostrEventsPubSub --retry"
  },
  "dependencies": {
    "@google-cloud/datastore": "^8.2.1",
    "@google-cloud/functions-framework": "^3.0.0",
    "@nostr-dev-kit/ndk": "^1.3.1",
    "nostr-tools": "^1.16.0",
    "openai": "^4.11.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "c8": "^8.0.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.0.0",
    "sinon": "^15.0.0",
    "supertest": "^6.0.0",
    "uuid": "^9.0.0"
  }
}
